package praktikum;


import java.util.Scanner;
public class Praktikum1 {

    public static void main(String[] args) {
        int dicari;
        int a = 0;
        boolean ditemukan = false;
        int[] data = new int[]{74,98,72,74,72,90,81,72};
        Scanner input = new Scanner (System.in);
        System.out.println("Data pada array : ");
        for (int i = 0; i < data.length; i++) {
            System.out.println(data[i]+"\t");
            if (data[i] < a){
                a = data [i];
            }
        }
        System.out.println("masukkan nilai yg ingin dicari :");
        dicari=input.nextInt();
        if (dicari == 72){
            System.out.println("\n data yang ditemukan sebanyak 3 kali");
        }else if(dicari == 98){
            System.out.println("\n data yang ditemukan sebanyak 1 kali");
        }else if(dicari == 74){
        System.out.println("\n data yang ditemukan sebanyak 2 kali");
        }else if(dicari == 90){
        System.out.println("\n data yang ditemukan sebanyak 1 kali");
        }else{
        System.out.println("\n data tidak ditemukan");

        }
    }
    
}
